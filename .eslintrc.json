{
    "env": {
        "browser": true,
        "jquery": true
    },
    "rules": {
        "indent": ["error", 2],
        "semi": "error",
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "quotes": ["warn", "single"]
    }
}